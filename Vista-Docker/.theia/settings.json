{
    "files.associations": {
        "*.m": "mumps"
    },
    "code-runner.executorMap": {
        "mumps": "sudo chmod 0600 /root/.ssh/id_rsa && sudo ssh -o \"StrictHostKeyChecking Off\" root@vistadash 'ydb <<< \"D ^$fileNameWithoutExt\"',
        "python": "sudo chmod 0600 /root/.ssh/id_rsa && sudo ssh -o \"StrictHostKeyChecking Off\" root@iknow 'python3.8 $dir/$fileName'
    }
}
